<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="SwitchButton">
        <attr name="selectedColor" format="color|reference"/>
        <attr name="textSize" format="dimension"/>
        <attr name="selectedTab" format="integer"/>
        <attr name="strokeWidth" format="dimension"/>
        <attr name="strokeRadius" format="dimension"/>
        <attr name="tabTexts" format="enum">
            <enum name="tabText1" value="left"/>
            <enum name="tabText2" value="center"/>
            <enum name="tabText3" value="right"/>
        </attr>
    </declare-styleable>










</resources>